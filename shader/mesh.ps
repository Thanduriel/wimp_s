#version 330

in vec4 vs_out_normal;
in vec2 vs_out_texCoords;

//out vec4 fragColor;
out vec4 gl_FragData[2];

void main()
{
	vec4 fragColor;
	fragColor = vs_out_normal;
	fragColor.w = 1;
	float cosTheta = clamp( dot( vs_out_normal, vec4(0,1,0,1) ), 0,1 );
	fragColor.rgb = vec3(0.4,0.3,0.6);
//	fragColor *= clamp(cosTheta, 0.2, 1);
	fragColor.a = 1;
	
	gl_FragData[0] = fragColor;
	gl_FragData[1] = vs_out_normal;
}