#version 330

in vec4 gs_color;
in vec2 gs_texCoord;

layout(location = 0) out vec4 out_FragColor;
layout(location = 1) out vec4 out_Normal;
void main()
{
	float circleDist = sqrt(max(0.0, 1.0 - dot(gs_texCoord, gs_texCoord)));
	if(circleDist == 0.0) discard;

	out_FragColor = gs_color;
	out_Normal = vec4(1,0,0,0);
}
